  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@mixcore/client-api": ["packages/client-api/src"]
